{
    "name": "habanatech/cartelon2.0",
    "type": "project",
    "license": "MIT",
    "description": "CartelON: Grafica cubana",
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "easycorp/easyadmin-bundle": "^2.3",
        "sensio/framework-extra-bundle": "^5.6.1",
        "symfony/asset": "^5.3.0",
        "symfony/console": "^5.3.0",
        "symfony/dotenv": "^5.3.0",
        "symfony/expression-language": "^5.3.0",
        "symfony/flex": "*",
        "symfony/form": "^5.3.0",
        "symfony/framework-bundle": "^5.3.0",
        "symfony/http-client": "^5.3.0",
        "symfony/intl": "^5.3.0",
        "symfony/monolog-bundle": "*",
        "symfony/orm-pack": "*",
        "symfony/process": "^5.3.0",
        "symfony/security-bundle": "^5.3.0",
        "symfony/serializer-pack": "*",
        "symfony/translation": "^5.3.0",
        "symfony/validator": "^5.3.0",
        "symfony/web-link": "^5.3.0",
        "symfony/yaml": "^5.3.0",
        "symfony/mailer": "^5.3.0",
        "symfony/messenger": "^5.3.0",
        "symfony/mime": "^5.3.0",
        "symfony/twig-bundle": "^5.3.0",
        "symfony/twig-pack": "*",
        "symfony/webpack-encore-bundle": "*",
        "twig/cssinliner-extra": "^3.3",
        "twig/inky-extra": "^3.3",
        "twig/markdown-extra": "^3.3"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.32",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    }
}
